  compatible = "arm,cortex-a15-gic";
  interrupt-controller;
  #interrupt-cells = <3>;
  arm,routable-irqs = <192>;
  reg = <0x48211000 0x1000>,
        <0x48212000 0x1000>,
        <0x48214000 0x2000>,
        <0x48216000 0x2000>;
  interrupts = <1 9 ((((1 << (2)) - 1) << 8) | 4)>;
 };





